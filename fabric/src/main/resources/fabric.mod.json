{
	"schemaVersion": 1,
	"id": "pandalib",
	"version": "${mod_version}",
	"name": "PandaLib",
	"description": "A multipurpose library with a Config API, Kotlin library support, a bit of utility code, and more.",
	"authors": [
		"The Panda Oliver"
	],
	"contact": {
		"issues": "https://github.com/PandaMods-Dev/PandaLib/issues",
		"sources": "https://github.com/PandaMods-Dev/PandaLib"
	},
	"license": "GNU LGPLv3",
	"icon": "assets/pandalib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "dev.pandasystems.pandalib.fabric.PandaLibFabric"
			}
		],
		"client": [
			{
				"value": "dev.pandasystems.pandalib.fabric.client.PandaLibClientFabric"
			}
		]
	},
	"mixins": [
		"pandalib.mixins.json",
		"pandalib-common.mixins.json"
	],
	"depends": {
		"fabric": "*",
		"fabric-api": "*",
		"minecraft": "1.21.4",
		"architectury": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/wjPt4vEfXb",
				"modmenu.kofi": "https://ko-fi.com/pandamods",
				"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/pandalib",
				"modmenu.modrinth": "https://modrinth.com/mod/pandalib",
				"modmenu.github_releases": "https://github.com/PandaMods-Dev/PandaLib/releases"
			},
			"badges": [
				"library"
			],
			"update_checker": true
		}
	}
}