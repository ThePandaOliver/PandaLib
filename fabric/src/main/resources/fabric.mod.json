{
  "schemaVersion": 1,
  "id": "pandalib",
  "version": "${version}",
  "name": "PandaLib",
  "description": "A multi purpose library with a Config API, Model and Animation API, Menu API and a bit of utility code.",
  "authors": [
		"Panda Mods/The Panda Oliver"
  ],
	"icon": "icon.png",
	"contact": {
		"issues": "https://github.com/PandaDap2006/PandaLib/issues",
		"sources": "https://github.com/PandaDap2006/PandaLib"
	},
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
			"me.pandamods.pandalib.fabric.PandaLibFabric"
    ],
		"client": [
			"me.pandamods.pandalib.fabric.PandaLibFabricClient"
		]
  },
  "mixins": [
		"pandalib.mixins.json",
    "pandalib-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabric-api": "*",
    "minecraft": ">=1.20",
    "architectury": ">=9.1.12"
  },
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/wjPt4vEfXb"
			}
		}
	}
}