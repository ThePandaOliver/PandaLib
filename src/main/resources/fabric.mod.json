{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		${mod_authors}
	],
	"contact": {
		"issues": "https://github.com/ThePandaOliver/PandaLib/issues",
		"sources": "https://github.com/ThePandaOliver/PandaLib"
	},
	"license": "${mod_license}",
	"icon": "assets/${mod_id}/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.pandasystems.pandalib.fabric.PandaLibFabric"
		],
		"client": [
			"dev.pandasystems.pandalib.fabric.client.PandaLibClientFabric"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json"
	],
	"depends": {
		"fabric": ">=${loader_version}",
		"fabric-api": "*",
		"minecraft": "${minecraft_version}"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/wjPt4vEfXb",
				"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/pandalib",
				"modmenu.modrinth": "https://modrinth.com/mod/pandalib",
				"modmenu.github_releases": "https://github.com/ThePandaOliver/PandaLib/releases"
			},
			"badges": [
				"library"
			],
			"update_checker": true
		}
	}
}